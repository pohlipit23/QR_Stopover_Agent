{
  "name": "qatar-stopover-ai-agent",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "build:production": "NODE_ENV=production astro check && astro build",
    "build:staging": "NODE_ENV=staging astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "check": "astro check",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "jest --testPathPatterns=e2e",
    "test:uat": "jest --testPathPatterns=UserAcceptanceTests",
    "test:e2e-live": "node scripts/e2e-test.js",
    "test:all": "npm run test && npm run test:uat && npm run security:audit",
    "deploy:staging": "npm run build:staging && wrangler pages deploy dist --env staging",
    "deploy:production": "npm run build:production && wrangler pages deploy dist --env production",
    "deploy:preview": "npm run build && wrangler pages deploy dist",
    "lint": "astro check",
    "lint:fix": "astro check --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist .astro node_modules/.cache",
    "precommit": "npm run lint && npm run test",
    "prepare": "npm run build",
    "security:audit": "node scripts/security-audit.js",
    "assets:upload": "node scripts/upload-assets.js",
    "assets:validate": "node scripts/upload-assets.js --validate",
    "cloudflare:setup": "npm run assets:validate && npm run assets:upload",
    "security:check": "npm audit && npm run security:audit",
    "security:fix": "npm audit fix"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.24",
    "@astrojs/check": "^0.9.4",
    "@astrojs/cloudflare": "^11.1.0",
    "@astrojs/react": "^3.6.2",
    "@astrojs/tailwind": "^5.1.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "ai": "^5.0.35",
    "astro": "^4.16.19",
    "hono": "^4.6.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.8.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "ts-jest": "^29.4.1"
  }
}
